<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="352dp"
        android:layout_height="129dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/custom_button1" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="352dp"
        android:layout_height="129dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/custom_button1" />

    <ImageButton
        android:id="@+id/btnback3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:backgroundTint="@color/background"
        android:src="@drawable/backicon"
        android:tint="@color/fontcolor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/backicon" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/itim"
        android:text="NOTIFICATIONS"
        android:textColor="@color/fontcolor"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="268dp"
        android:layout_height="66dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/itim"
        android:text="It's time for vaccination of your puppy"
        android:textColor="#50327A"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="248dp"
        android:layout_height="58dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="250dp"
        android:fontFamily="@font/itim"
        android:text="It's time for deworming your puppy"
        android:textColor="#50327A"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="305dp"
        android:layout_height="47dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="160dp"
        android:fontFamily="@font/itim"
        android:text="Rabies + Leptospiro vaccination due date is on 24/03/2024"
        android:textSize="19sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/itim"
        android:text="Deworming due date is on 24/03/2024"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/notifi" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:srcCompat="@drawable/notifi" />

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="415dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="580dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/navigation_bar"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/btnhome"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/navigation_bar"
            android:src="@drawable/home"
            android:tint="@color/white"
            tools:srcCompat="@drawable/home" />

        <ImageButton
            android:id="@+id/btnnoti"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/navigation_bar"
            android:backgroundTint="@color/appointback"
            android:elevation="10dp"
            android:src="@drawable/alarmicon"
            android:tint="@color/fontcolor"
            tools:srcCompat="@drawable/alarmicon" />

        <ImageButton
            android:id="@+id/btntips"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/navigation_bar"
            android:src="@drawable/ideaicon"
            android:tint="#FFFFFF"
            tools:srcCompat="@drawable/ideaicon" />

        <ImageButton
            android:id="@+id/btnprofile"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/navigation_bar"
            android:src="@drawable/footprint"
            android:tint="#FFFFFF"
            tools:srcCompat="@drawable/footprint" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>